<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="selvishop">
  

  
    <select id="order_total_count" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
            select count(*) as order_cnt from prdt_order
        ]]>
    </select>
    
    <select id="product_total_count" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
            select count(*) as product_cnt from product
        ]]>
    </select>
  
    <select id="member_total_count" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
            select count(*) as member_cnt from member
        ]]>
    </select>
    
    <select id="board_total_count" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
            select count(*) as board_cnt from board
        ]]>
    </select>
    
    
    
  
  </mapper>
  