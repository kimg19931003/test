  <mapper namespace="selvishop">
  

  
    <select id="admin_login_post" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
          select * from admin_member where admin_mem_id = '${admin_mem_id}' and admin_mem_pass = '${admin_mem_pass}'
        ]]>
