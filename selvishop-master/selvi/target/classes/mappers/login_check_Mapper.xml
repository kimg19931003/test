<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="selvishop">
  
    
  
    <select id="login_check" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
           select * from member where mem_id='${mem_id}' and mem_pass = '${mem_pass}'
        ]]>
    </select>
    


	<select id="login_id_exist" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
           select * from member where mem_id='${mem_id}'
        ]]>
    </select>
  
  
  
  
  </mapper>
  